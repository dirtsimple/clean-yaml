{
	"name" : "dirtsimple/clean-yaml",
	"type" : "library",
	"description" : "Dump diff-friendly, readable YAML",
	"license" : "MIT",
	"require" : {
		"php" : ">=5.6",
		"symfony/yaml": "^3.4"
	},
	"keywords" : [
		"yaml"
	],
	"autoload" : {
		"psr-4" : {
			"dirtsimple\\" : "src"
		}
	},
	"require-dev" : {
		"peridot-php/leo" : "~1.6",
		"peridot-php/peridot" : "~1.19",
		"peridot-php/peridot-watcher-plugin": "^1.3"
	},
	"config" : {
		"process-timeout": 0
	},
	"scripts" : {
		"watch": "peridot --force-colors --watch specs",
		"test" : "peridot specs"
	}
}